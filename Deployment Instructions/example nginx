http {
    include       mime.types;
    default_type  application/octet-stream;

    sendfile        on;
    keepalive_timeout  65;

    server {
        listen       9000;
        server_name  lovetoeat.it;

        root   /Users/zeeshan/projects/LoveToEatItFrontEnd/dist;
        index index.html;

        location / {
            try_files  $uri /index.html;
        }
    }